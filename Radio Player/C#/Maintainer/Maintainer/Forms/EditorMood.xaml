<Window x:Class="Maintainer.Forms.EditorMood"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maintainer.Forms"
        mc:Ignorable="d"
        Title="MoodEditor" Height="126.316" Width="300">
    <Grid>
        <Label Content="ID:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Description:" HorizontalAlignment="Left" Margin="10,36,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textbox_description" HorizontalAlignment="Left" Height="23" Margin="83,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199"/>
        <TextBox x:Name="textbox_id" HorizontalAlignment="Left" Height="23" Margin="83,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="199" IsEnabled="False"/>
        <Button x:Name="button_apply" Content="Apply" HorizontalAlignment="Left" Margin="207,64,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="button_apply_Click"/>
        <Button x:Name="button_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="127,64,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="button_cancel_Click"/>

    </Grid>
</Window>
