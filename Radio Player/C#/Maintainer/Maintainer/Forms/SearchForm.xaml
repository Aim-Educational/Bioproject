<Window x:Class="Maintainer.Forms.SearchForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Maintainer.Forms"
        mc:Ignorable="d"
        Title="SearchForm" Height="300" Width="300">
    <Grid>
        <ListView x:Name="list" HorizontalAlignment="Stretch" Height="auto" VerticalAlignment="Stretch" Width="auto">
            <ListView.View>
                <GridView AllowsColumnReorder="False">
                    <GridViewColumn Header="ID" Width="100" DisplayMemberBinding="{Binding objectID}"/>
                    <GridViewColumn Header="Description" Width="200" DisplayMemberBinding="{Binding objectDescription}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
